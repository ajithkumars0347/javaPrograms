package com.kodnest.pattern.level7.assignments25patterns;

public class Pattern24 {
public static void main(String[] args) {
	for(int i=1;i<=5;i++) {
		for(int j=1;j<=i;j++) {
			System.out.print("*");
		}
	}
	for(int i=5;i<=1;i--) {
		for(int j=2^i-2;j>=1;j--) {
			System.out.print(" ");
		}
		for(int k=5;k>=i;i--) {
		System.out.print("*");	
		}
		System.out.println();
	}
}
}
