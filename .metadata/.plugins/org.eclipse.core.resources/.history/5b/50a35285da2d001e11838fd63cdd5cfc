package com.kodnest.pattern.level7.assignments25patterns;

public class Pattern5 {
public static void main(String[] args) {
	for(char i='A';i<='E';i++) {
		for(int k='B';k>=i;k--) {
			System.out.print(" ");
		}
		for(int j=1;j<=(i*2)-1;j++) {
			System.out.print(i);
		}
		System.out.println();
	}
}
}
