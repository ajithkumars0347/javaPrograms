package com.kodnest.pattern.level7.assignments25patterns;

public class Pattern9 {
public static void main(String[] args) {
	for(int i=0;i<=4;i++) {
		for(int k=4;k>=i;k--) {
			System.out.print(" ");
		}
        for(int s=i;s>=0;s--){
              System.out.println(s);
        }
		System.out.println();
	}

}
}
