package com.kodnest.pattern.level7.assignments25patterns;

public class Pattern15 {
public static void main(String[] args) {
	char ch='A';
	for(int i=0;i<=5;i++) {
		for(int j=0;j<=6;j++) {
			System.out.print(ch);
			ch++;
		}
		for(int s=0;s<2*i;s++) {
			System.out.print(" ");
		}
		ch--;
		for(int k=0;k<6-i;k++) {
			System.out.print(ch);
			ch--;
		}
		System.out.println();
	}
}
}
